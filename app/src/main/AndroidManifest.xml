<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.first.miso"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MainActivity"/>
        <activity android:name=".choose_skiplace"/>
        <activity android:name=".choose_date"/>
        <activity android:name=".check_info"/>
        <activity android:name=".check_use_money"/>
        <activity android:name=".move_ski_site"/>
        <activity android:name=".reserve_cancel"/>
        <activity android:name=".withdraw"/>
        <activity android:name=".withdraw_yes_click"/>
        <activity android:name=".new_register"/>
        <activity android:name=".reserve_info"/>

    </application>
    <uses-permission android:name="android.permission.INTERNET"/>

</manifest>