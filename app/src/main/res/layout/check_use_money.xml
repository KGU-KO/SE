<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/move_ski_site"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.first.miso.move_ski_site">

    <Button
        android:text="곤지암스키장"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/usemoney_button1"
        android:layout_alignParentEnd="true" />

    <Button
        android:text="알펜시아스키장"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/usemoney_button2"
        android:layout_below="@+id/usemoney_button1"
        android:layout_alignParentStart="true" />

    <Button
        android:text="덕유산스키장"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/usemoney_button3"
        android:layout_below="@+id/usemoney_button2"
        android:layout_alignParentStart="true" />

    <Button
        android:text="비발디파크"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/usemoney_button4"
        android:layout_below="@+id/usemoney_button3"
        android:layout_alignParentStart="true" />

    <Button
        android:text="Main"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="76dp"
        android:id="@+id/usemoney_main" />


</RelativeLayout>